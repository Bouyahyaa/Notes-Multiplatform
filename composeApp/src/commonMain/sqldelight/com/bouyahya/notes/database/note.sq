CREATE TABLE NoteEntity(
    id TEXT NOT NULL PRIMARY KEY,
    title TEXT,
    description TEXT
);

getAllNotes:
SELECT *
FROM NoteEntity;

getNote:
SELECT *
FROM NoteEntity
WHERE id == :id;

insertNote:
INSERT OR REPLACE
INTO NoteEntity(
    id,
    title,
    description
)
VALUES ?;

deleteNote:
DELETE FROM NoteEntity
WHERE id = ?;